        <Parameter name="y min" type="double" value="-1.5" />
        <Parameter name="y max" type="double" value="0.0" />
      </ParameterList>
    </ParameterList>

    <ParameterList name="Permittivity">
      <Parameter name="Permittivity Type" type="string" value="Block Dependent" />
