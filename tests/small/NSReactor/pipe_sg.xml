<ParameterList>
  <ParameterList name="Problem">
    <Parameter name="Phalanx Graph Visualization Detail" type="int" value="0"/>
    <Parameter name="Name" type="string" value="NavierStokes 2D"/>
    <ParameterList name="Heat">
      <Parameter name="Variable Type" type="string" value="DOF"/>
    </ParameterList>
    <!--ParameterList name="Thermal Conductivity">
      <Parameter name="Type" type="string" value="Constant"/>
      <Parameter name="Value" type="double" value="1e-1"/>
    </ParameterList>
    <ParameterList name="Viscosity">
      <Parameter name="Type" type="string" value="Truncated KL Expansion"/>
      <Parameter name="Number of KL Terms" type="int" value="2"/>
      <Parameter name="Mean" type="double" value="1.0"/>
      <Parameter name="Standard Deviation" type="double" value="0.25"/>
      <Parameter name="Domain Lower Bounds" type="string" value="{0.0, 0.45}"/>
      <Parameter name="Domain Upper Bounds" type="string" value="{1.0, 0.55}"/>
      <Parameter name="Correlation Lengths" type="string" value="{0.1, 0.01}"/>
    </ParameterList-->
    <ParameterList name="Thermal Conductivity">
      <Parameter name="Type" type="string" value="Truncated KL Expansion"/>
      <Parameter name="Number of KL Terms" type="int" value="2"/>
      <Parameter name="Mean" type="double" value="1e-1"/>
      <Parameter name="Standard Deviation" type="double" value="5e-2"/>
      <Parameter name="Domain Lower Bounds" type="string" value="{0.0, 0.45}"/>
      <Parameter name="Domain Upper Bounds" type="string" value="{1.0, 0.55}"/>
      <Parameter name="Correlation Lengths" type="string" value="{0.1, 0.01}"/>
    </ParameterList>
    <ParameterList name="Dirichlet BCs">     
      <Parameter name="DBC on NS top for DOF ux" type="double" value="0.0"/>
      <Parameter name="DBC on NS top for DOF uy" type="double" value="0.0"/>
      <Parameter name="DBC on NS top for DOF T" type="double" value="0.0"/>
      <Parameter name="DBC on NS bottom for DOF ux" type="double" value="0.0"/>
      <Parameter name="DBC on NS bottom for DOF uy" type="double" value="0.0"/>
      <Parameter name="DBC on NS bottom for DOF T" type="double" value="0.0"/>
      <Parameter name="DBC on NS inlet for DOF ux" type="double" value="100.0"/>
      <Parameter name="DBC on NS inlet for DOF uy" type="double" value="0.0"/>
      <!--Parameter name="DBC on NS inlet for DOF T" type="double" value="100.0"/-->
      <Parameter name="DBC on NS outlet for DOF uy" type="double" value="0.0"/>
    </ParameterList>
    <ParameterList name="Neumann BCs">     
      <Parameter name="NBC on SS inlet_ss for DOF T set dudn" type="Array(double)" value="{-7.0}"/>
      <Parameter name="NBC on SS outlet_ss for DOF T set dudn" type="Array(double)" value="{32.0}"/>
    </ParameterList>
    <ParameterList name="Parameters">
      <Parameter name="Number of Parameter Vectors" type="int" value="2"/>
      <ParameterList name="Parameter Vector 0">
	<Parameter name="Number" type="int" value="2"/>
	<Parameter name="Parameter 0" type="string" value="NBC on SS inlet_ss for DOF T set dudn"/>
	<Parameter name="Parameter 1" type="string" value="NBC on SS outlet_ss for DOF T set dudn"/>
      </ParameterList>
      <ParameterList name="Parameter Vector 1">
	<Parameter name="Number" type="int" value="2"/>
	<Parameter name="Parameter 0" type="string" value="Thermal Conductivity KL Random Variable 0"/>
	<Parameter name="Parameter 1" type="string" value="Thermal Conductivity KL Random Variable 1"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Response Functions">
      <Parameter name="Number of Response Vectors" type="int" value="1"/>
      <ParameterList name="Response Vector 0">
	<Parameter name="Name" type="string" value="Aggregate Responses"/>
        <Parameter name="Number" type="int" value="2"/>
        <Parameter name="Response 0" type="string" value="PHAL Field Integral"/>
	<ParameterList name="ResponseParams 0">
          <Parameter name="Field Name" type="string" value="Temperature"/>
	  <Parameter name="x min" type="double" value="0.0"/>
	  <!--Parameter name="x max" type="double" value="0.01"/-->
	  <Parameter name="x max" type="double" value="0.1"/>
	  <Parameter name="y min" type="double" value="0.45"/>
	  <Parameter name="y max" type="double" value="0.55"/>
	  <!--Parameter name="Length Scaling" type="double" value="31.6227766"/-->
	  <Parameter name="Length Scaling" type="double" value="10.0"/>
        </ParameterList>
	<Parameter name="Response 1" type="string" value="PHAL Field Integral"/>
        <ParameterList name="ResponseParams 1">
	  <Parameter name="Field Name" type="string" value="Temperature"/>
	  <!--Parameter name="x min" type="double" value="0.99"/-->
	  <Parameter name="x min" type="double" value="0.90"/>
	  <Parameter name="x max" type="double" value="1.00"/>
	  <Parameter name="y min" type="double" value="0.45"/>
	  <Parameter name="y max" type="double" value="0.55"/>
	  <!--Parameter name="Length Scaling" type="double" value="31.6227766"/-->
	  <Parameter name="Length Scaling" type="double" value="10.0"/>
	</ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>
  <ParameterList name="Discretization">
    <Parameter name="Method" type="string" value="Ioss"/>
    <Parameter name="Workset Size" type="int" value="10"/>
    <Parameter name="Exodus Input File Name" type="string" value="pipe_coarse.exo"/>
    <Parameter name="Exodus Output File Name" type="string" value="pipe_coarse_sg-out.exo"/>
  </ParameterList>
  <ParameterList name="Regression Results">
    <Parameter  name="Number of Comparisons" type="int" value="0"/>
    <Parameter  name="Test Values" type="Array(double)" value="{1.358}"/>
    <Parameter  name="Relative Tolerance" type="double" value="1.0e-3"/>
    <Parameter  name="Number of Sensitivity Comparisons" type="int" value="0"/>
    <Parameter  name="Sensitivity Test Values 0" type="Array(double)"
		value="{1.358}"/>
    <Parameter  name="Number of Dakota Comparisons" type="int" value="0"/>
  </ParameterList>
  <ParameterList name="Piro">
    <Parameter name="Print Convergence Stats" type="bool" value="0"/>
    <ParameterList name="Stochastic Galerkin">
      <Parameter name="Save Moments" type="int" value="2"/>
      <ParameterList name="SG Parameters">
	<Parameter name="Number of SG Parameter Vectors" type="int" value="1"/>
	<Parameter name="Set Initial SG Parameters" type="bool" value="0"/>
	<ParameterList name="SG Parameter Vector 0">
	  <Parameter name="Parameter Vector Index" type="int" value="1"/>
	</ParameterList>
      </ParameterList>
      <ParameterList name="Basis">
	<Parameter name="Isotropic" type="bool" value="true"/>
	<Parameter name="Type" type="string" value="Legendre"/>
	<Parameter name="Order" type="int" value="2"/>
	<Parameter name="Dimension" type="int" value="4"/>
	<Parameter name="Normalize" type="bool" value="true"/>
	<Parameter name="Cijk Drop Tolerance" type="double" value="1e-12"/>
      </ParameterList>
      <ParameterList name="Quadrature">
	<Parameter name="Type" type="string" value="Sparse Grid"/>
      </ParameterList>
      <!--Parameter name="SG Method" type="string" value="Global"/-->
      <Parameter name="SG Method" type="string" value="Non-intrusive"/>
      <ParameterList name="Expansion">
	<Parameter name="Type" type="string" value="Quadrature"/>
      </ParameterList>
      <Parameter name="Triple Product Size" type="string" 
		 value="Full"/>
      <ParameterList name="SG Solver Parameters">    
	<Parameter name="SG Solver Algorithm" type="string" value="Krylov"/>    
	<Parameter name="Jacobian Expansion Type" type="string" 
		   value="Full"/>
	<Parameter name="Parameter Expansion Type" type="string" 
		   value="Full"/>
	<ParameterList name="SG Operator">
	  <Parameter name="Operator Method" type="string" value="Matrix Free"/>
	</ParameterList>
	<ParameterList name="SG Preconditioner">
	  <Parameter name="Preconditioner Method" type="string" value="Approximate Gauss-Seidel"/>
	  <Parameter name="Symmetric Gauss-Seidel" type="bool" value="0"/>
	  <Parameter name="Mean Preconditioner Type" type="string" value="Ifpack"/>
	  <ParameterList name="Mean Preconditioner Parameters">
	    <Parameter name="fact: level-of-fill" type="int" value="4"/>
	  </ParameterList>
	</ParameterList>
      </ParameterList>
    </ParameterList>
    <ParameterList name="NOX">
      <ParameterList name="Status Tests">
	<Parameter name="Test Type" type="string" value="Combo"/>
	<Parameter name="Combo Type" type="string" value="OR"/>
	<Parameter name="Number of Tests" type="int" value="2"/>
	<ParameterList name="Test 0">
	  <Parameter name="Test Type" type="string" value="Combo"/>
	  <Parameter name="Combo Type" type="string" value="AND"/>
	  <Parameter name="Number of Tests" type="int" value="2"/>
	  <ParameterList name="Test 0">
	    <Parameter name="Test Type" type="string" value="NormF"/>
	    <Parameter name="Norm Type" type="string" value="Two Norm"/>
	    <Parameter name="Scale Type" type="string" value="Scaled"/>
	    <Parameter name="Tolerance" type="double" value="1e-7"/>
	  </ParameterList>
	  <ParameterList name="Test 1">
	    <Parameter name="Test Type" type="string" value="NormWRMS"/>
	    <Parameter name="Absolute Tolerance" type="double" value="1e-3"/>
	    <Parameter name="Relative Tolerance" type="double" value="1e-3"/>
	  </ParameterList>
	</ParameterList>
	<ParameterList name="Test 1">
	  <Parameter name="Test Type" type="string" value="MaxIters"/>
	  <Parameter name="Maximum Iterations" type="int" value="10"/>
	</ParameterList>
      </ParameterList>
      <ParameterList name="Direction">
	<Parameter name="Method" type="string" value="Newton"/>
	<ParameterList name="Newton">
	  <Parameter name="Forcing Term Method" type="string" value="Constant"/>
	  <ParameterList name="Linear Solver">
	    <Parameter name="Write Linear System" type="bool" value="false"/>
	  </ParameterList>
	  <ParameterList name="Stratimikos Linear Solver">
	    <ParameterList name="NOX Stratimikos Options">
	    </ParameterList>
	    <ParameterList name="Stratimikos">
	      <Parameter name="Linear Solver Type" type="string" value="AztecOO"/>
	      <ParameterList name="Linear Solver Types">
		<ParameterList name="AztecOO">
		  <ParameterList name="Forward Solve"> 
		    <ParameterList name="AztecOO Settings">
		      <Parameter name="Aztec Solver" type="string" value="GMRES"/>
		      <Parameter name="Convergence Test" type="string" value="r0"/>
		      <Parameter name="Size of Krylov Subspace" type="int" value="50"/>
		      <Parameter name="Output Frequency" type="int" value="20"/>
		    </ParameterList>
		    <Parameter name="Max Iterations" type="int" value="500"/>
		    <Parameter name="Tolerance" type="double" value="1e-6"/>
		  </ParameterList>
		  <ParameterList name="VerboseObject"> 
		    <Parameter name="Verbosity Level" type="string" 
			       value="none"/>
		  </ParameterList>
		</ParameterList>
	      </ParameterList>
	      <Parameter name="Preconditioner Type" type="string" value="Ifpack"/>
	      <ParameterList name="Preconditioner Types">
		<ParameterList name="ML">
		  <Parameter name="Base Method Defaults" type="string" 
			     value="none"/>
		  <ParameterList name="ML Settings">
		    <Parameter name="default values" type="string" value="SA"/>
		    <Parameter name="smoother: type" type="string" 
			       value="ML symmetric Gauss-Seidel"/>
		    <Parameter name="smoother: pre or post" type="string" 
			       value="both"/>
		    <Parameter name="coarse: type" type="string" 
			       value="Amesos-KLU"/>
		    <Parameter name="PDE equations" type="int" 
			       value="4"/>
		  </ParameterList>
		</ParameterList>
	      </ParameterList>
	    </ParameterList>
	  </ParameterList>

	  <Parameter name="Rescue Bad Newton Solve" type="bool" value="1"/>
	</ParameterList>
      </ParameterList>
      <ParameterList name="Line Search">
	<ParameterList name="Full Step">
	  <Parameter name="Full Step" type="double" value="1"/>
	</ParameterList>
	<Parameter name="Method" type="string" value="Full Step"/>
      </ParameterList>
      <Parameter name="Nonlinear Solver" type="string" value="Line Search Based"/>
      <ParameterList name="Printing">
	<ParameterList name="Output Information">
	  <Parameter name="Error" type="bool" value="1"/>
	  <Parameter name="Warning" type="bool" value="1"/>
	  <Parameter name="Outer Iteration" type="bool" value="0"/>
	  <Parameter name="Parameters" type="bool" value="0"/>
	  <Parameter name="Details" type="bool" value="0"/>
	  <Parameter name="Linear Solver Details" type="bool" value="0"/>
	</ParameterList>
	<Parameter name="Output Precision" type="int" value="3"/>
	<Parameter name="Output Processor" type="int" value="0"/>
      </ParameterList>
      <ParameterList name="Solver Options">
	<Parameter name="Status Test Check Type" type="string" value="Minimal"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>
</ParameterList>
